<!-- 消息 -->
<template name="personalizations">
  <view class="Personalization">
    <view class="personalization">
      <view class="table">
        <view class="tr">
          <view class="td"></view>
          <view class="td1">姓名</view>
          <view class="td2">{{name}}</view>
        </view>
        
        <view class="tr">
          <view class="td"></view>
          <view class="td1">生日</view>
          <view class="td2">{{birthday}}</view>
        </view>

        <view class="tr">
          <view class="td"></view>
          <view class="td1">家乡</view>
          <view class="td2">{{hometown}}</view>
        </view>
      </view>
      
      <view class="table">
        <view class="tr">
          <view class="td"></view>
          <view class="td1">院校</view>
          <view class="td2"></view>
        </view>
        <view class="tr">
          <view class="td"></view>
          <view class="td1">专业</view>
          <view class="td2"></view>
        </view>
      
      </view>
      
      <view class="table">
        <view class="tr">
          <view class="td"></view>
          <view class="td1">个性签名</view>
          <view class="td2">{{description}}</view>
        </view>
       
      </view>
    
    </view>
  </view>
</template>
